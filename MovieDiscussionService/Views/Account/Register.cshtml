@model Common.DTOs.RegisterDTO
<h2>Register</h2>
@using (Html.BeginForm())
{
    @Html.LabelFor(m => m.FullName) @Html.TextBoxFor(m => m.FullName)
    @Html.LabelFor(m => m.Gender) @Html.TextBoxFor(m => m.Gender)
    @Html.LabelFor(m => m.Country) @Html.TextBoxFor(m => m.Country)
    @Html.LabelFor(m => m.City) @Html.TextBoxFor(m => m.City)
    @Html.LabelFor(m => m.Address) @Html.TextBoxFor(m => m.Address)
    @Html.LabelFor(m => m.Email) @Html.TextBoxFor(m => m.Email)
    @Html.LabelFor(m => m.Password) @Html.PasswordFor(m => m.Password)
    @Html.LabelFor(m => m.PhotoUrl) @Html.TextBoxFor(m => m.PhotoUrl)
    <button type="submit">Register</button>
}
